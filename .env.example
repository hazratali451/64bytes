# Brevo API Key
# Get your API key from https://app.brevo.com/settings/keys/api
BREVO_API_KEY=your_brevo_api_key_here

# Brevo Sender Email
# Make sure to verify this domain in your Brevo account
BREVO_SENDER_EMAIL=your_sender_email@example.com

# Form Receiving Email
BREVO_RECEIVING_EMAIL=your_receiving_email@example.com

# Receiver Name
BREVO_RECEIVER_NAME=Your Name